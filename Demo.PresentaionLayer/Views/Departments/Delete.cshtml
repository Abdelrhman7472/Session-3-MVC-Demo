@model Department
@{
    ViewData["Title"] = "Delete";
}


<form method="post" asp-action="Delete">
    <h1 class="alert-danger"> Delete @Model.Name Department </h1>
    <div class="form-group mt-3">
        <label asp-for="Code"></label>
        <input readonly class="form-control" asp-for="Code" />
    </div>
    <div class="form-group">
        <label asp-for="Name"></label>
        <input readonly class="form-control" asp-for="Name" />

    </div>
    <div class="form-group">
        <label asp-for="DateOfCreation"></label>
        <input readonly class="form-control" asp-for="DateOfCreation" />
    </div>

    <div class="mt-3">
        <button class="btn btn-danger" type="submit">Delete</button>
        <a class="btn btn-secondary" asp-action="Index">Back</a>
    </div>

</form>
